{
  "manifest_version": 2,
  "name": "Slack Redirect",
  "version": "0.1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["webRequest", "webRequestBlocking", "https://*.slack.com/*"],
  "browser_specific_settings": {
    "gecko": {
      "id": "slack_redirect@oskarkook"
    }
  }
}
